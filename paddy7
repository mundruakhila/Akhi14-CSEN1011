#include <stdio.h>
int main()
{ 
    char t;
    int a,b,d;
    scanf("%c %d %d",&t,&a,&b);
    for(int i=0;i<a;i++)
    {
        for(int j=0;j<b;j++)
        {
            if(j%2==0)
                printf("%c ",t);
            else if(j%2!=0 && t=='B')
               printf("%c ",t+10);
            else
               printf("%c ",t-10);
        }
        printf("\n");
    }
}
